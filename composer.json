{
    "name": "shibuyakosuke/laravel-jet-adminlte",
    "description": "Easy AdminLTE integration with Laravel-jetstream",
    "keywords": [
        "laravel",
        "jetstream",
        "adminlte",
        "admin",
        "administrator",
        "livewire"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ShibuyaKosuke",
            "email": "github@shibuyakosuke.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "laravel/framework": ">=8.0",
        "almasaeed2010/adminlte": "3.1.*",
        "livewire/livewire": "^2.0",
        "laravel/jetstream": "^2.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.85",
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.21",
        "squizlabs/php_codesniffer": "^3.6",
        "nunomaduro/collision": "^5.10"
    },
    "autoload": {
        "psr-4": {
            "ShibuyaKosuke\\LaravelJetAdminlte\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ShibuyaKosuke\\LaravelJetAdminlte\\Providers\\ServiceProvider"
            ],
            "aliases": {
                "JetAdminLte": "ShibuyaKosuke\\LaravelJetAdminlte\\Facades\\JetAdminLte"
            }
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ]
    }
}
